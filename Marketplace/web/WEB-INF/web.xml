<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>Realtime data marketplace</display-name>
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>market.servlet.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupHandler</servlet-name>
        <servlet-class>market.servlet.SignupHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Default</servlet-name>
        <servlet-class>market.servlet.Default</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>market.servlet.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>market.servlet.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>market.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>market.servlet.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>market.servlet.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Service</servlet-name>
        <servlet-class>market.servlet.Service</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Subscribe</servlet-name>
        <servlet-class>market.servlet.Subscribe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubscribeHandler</servlet-name>
        <servlet-class>market.servlet.SubscribeHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Analytics</servlet-name>
        <servlet-class>market.servlet.Analytics</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Unsubscribe</servlet-name>
        <servlet-class>market.servlet.Unsubscribe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LatencyViewer</servlet-name>
        <servlet-class>market.servlet.LatencyViewer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>market.servlet.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PricingService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Default</servlet-name>
        <url-pattern>/default</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignupHandler</servlet-name>
        <url-pattern>/signuphandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/uploadservice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Service</servlet-name>
        <url-pattern>/service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Subscribe</servlet-name>
        <url-pattern>/subscribe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubscribeHandler</servlet-name>
        <url-pattern>/subscribehandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Analytics</servlet-name>
        <url-pattern>/analytics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Unsubscribe</servlet-name>
        <url-pattern>/unsubscribe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LatencyViewer</servlet-name>
        <url-pattern>/latencyviewer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PricingService</servlet-name>
        <url-pattern>/PricingService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
